{
    "name"        : "src-run/src.run",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://src.run/web-app",
    "description" : "simple redirection application written using the Silix framework",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://src.run/web-app/issues",
        "source" : "https://src.run/web-app/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@src.run",
            "homepage" : "https://src.run/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "silex/silex"            : "^1.1",
        "silex/web-profiler"     : "^1.0",
        "symfony/browser-kit"    : "^2.8||^3.0",
        "symfony/console"        : "^2.8||^3.0",
        "symfony/config"         : "^2.8||^3.0",
        "symfony/css-selector"   : "^2.8||^3.0",
        "symfony/dom-crawler"    : "^2.8||^3.0",
        "symfony/filesystem"     : "^2.8||^3.0",
        "symfony/finder"         : "^2.8||^3.0",
        "symfony/form"           : "^2.8||^3.0",
        "symfony/locale"         : "^2.8||^3.0",
        "symfony/process"        : "^2.8||^3.0",
        "symfony/serializer"     : "^2.8||^3.0",
        "symfony/translation"    : "^2.8||^3.0",
        "symfony/validator"      : "^2.8||^3.0",
        "symfony/yaml"           : "^2.8||^3.0",
        "symfony/monolog-bridge" : "^2.8||^3.0",
        "symfony/twig-bridge"    : "^2.8||^3.0",
        "twig/twig"              : "^1.11",
        "monolog/monolog"        : "^1.6",
        "knplabs/github-api"     : "^1.4",
        "eloquent/lcs"           : "^2.0",
        "mheap/silex-memcache"   : "dev-master"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "^5.0",
        "satooshi/php-coveralls" : "*",
        "sami/sami"              : "*"
    },
    "scripts": {
        "post-install-cmd" : "SR\\Composer\\Script::install",
        "post-update-cmd"  : "SR\\Composer\\Script::update"
    },
    "autoload" : {
        "psr-4" : {
            "SR\\" : "src/"
        }
    },
    "preferred-install" : "auto",
    "minimum-stability" : "stable",
    "config" : {
        "bin-dir" : "bin"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
